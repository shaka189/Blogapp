<h1>Post</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@post) do |f| %>


      <%= f.label :displaytitle %>
      <%= f.text_field :displaytitle , class: 'form-control'%>

      <%= f.label :body %>
      <%= f.text_field :body , class: 'form-control'%>

      <%= f.label :image %>
      <%= f.text_field :image , class: 'form-control'%>

      <%= f.submit "Create my post", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

